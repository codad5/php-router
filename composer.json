{
  "name": "codad5/php-router",
  "description": "A simple express-style router for PHP",
  "version": "2.0.5",
  "type": "library",
  "keywords": [
    "router",
    "php",
    "express",
    "api"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Codad5\\PhpRouter\\": "src/"
    }
  },
  "homepage": "https://github.com/codad5/php-router",
  "authors": [
    {
      "name": "Ayodeji O",
      "email": "osasonaayodeji1@gmail.com"
    },
    {
      "name": "Chibueze A",
      "email": "aniezeoformic@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": ">=7.4",
    "firebase/php-jwt": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "codad5/php-inex": "^1.0"
  },
  "scripts": {
    "start:dev": "php -S localhost:20000 -t examples",
    "test": "vendor/bin/phpunit --verbose __tests__",
    "test:watch": "vendor/bin/phpunit --verbose --watch __tests__"
  }
}
